  success: function(result) {

  },
});

ADD AN IMAGE

$.ajax({
  url: "http://845edac5.ngrok.io/data/",
  method: "POST",
  data: {'username': [username],
         'lat': [latitude degrees],
         'lon': [longitude degrees],
         'rating': [rating],
         'serializedImage': [base64 encoded image data],
  },
  success: function(result) {

  },
});

