});

CREATE A USER

$.ajax({
  url: "http://845edac5.ngrok.io/users/",
  method: "POST",
  data: {'username': [username],
         'password': [password],
         'name':  [name]
  },
  success: function(result) {

  },
});

SEARCH FOR users

$.ajax({
  url: "http://845edac5.ngrok.io/users/",
  method: "GET",
  data: {'username': [username],
         'lat': [latitude degrees],
         'lon':  [longitude degrees],
         'quant': [how many you want],
  },
  success: function(result) {

  },
});
